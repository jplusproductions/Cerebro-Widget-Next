{
  "name": "cerebro-widget-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn env:local && next dev --experimental-https",
    "dev:http": "yarn env:local && next dev",
    "dev:https": "yarn env:local && next dev --experimental-https",
    "dev:remote-dev": "yarn env:dev && next dev --experimental-https",
    "dev:remote-prod": "yarn env:prod && next dev --experimental-https",
    "build": "next build",
    "build:dev": "yarn env:dev && next build",
    "build:prod": "yarn env:prod && next build",
    "build:local": "yarn env:local && next build",
    "db:clear": "rimraf ./prisma/zod",
    "db:init-dev": "yarn env:dev && tsx ./prisma/runDBInitialization.ts",
    "db:init-prod": "yarn env:prod && tsx ./prisma/runDBInitialization.ts",
    "db:init:local": "yarn env:local && tsx ./prisma/runDBInitialization.ts",
    "db:generate": "yarn env:local && prisma generate",
    "db:migrate": "yarn env:local && prisma migrate dev --name \"@(date +%s)\"",
    "db:migrate-dev": "yarn env:dev && tsx ./prisma/runDBMigration.ts",
    "db:migrate-prod": "yarn env:prod && tsx ./prisma/runDBMigration.ts",
    "db:reset": "rimraf ./prisma/zod && yarn env:local && prisma migrate reset",
    "db:reset-dev": "rimraf ./prisma/zod && yarn env:dev && prisma migrate reset",
    "env:dev": "cross-env NEXT_PUBLIC_BUILD_ENV=dev tsx apps-configs/env-builder.ts",
    "env:prod": "cross-env NEXT_PUBLIC_BUILD_ENV=prod tsx apps-configs/env-builder.ts",
    "env:local": "cross-env NEXT_PUBLIC_BUILD_ENV=local tsx apps-configs/env-builder.ts",
    "start": "next start",
    "lint": "eslint",
    "format": "eslint --fix"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "7",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-table": "^8.21.3",
    "@trpc/client": "^11.10.0",
    "@trpc/server": "^11.10.0",
    "@trpc/tanstack-react-query": "^11.10.0",
    "axios": "^1.13.5",
    "better-auth": "^1.4.19",
    "chalk-scripts": "^2.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.3.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "superjson": "^2.2.6",
    "swagger-ui-react": "^5.31.2",
    "trpc-swagger": "^2.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "7",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
